<%= form_tag "/teams/#{@id}", method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>

  <%= label_tag :creature %>
  <%= text_field_tag :creature %><br>

  <%= label_tag :state %>
  <%= text_field_tag :state %><br>

  <%= label_tag :sport %>
  <%= text_field_tag :sport %><br>

  <%= submit_tag 'Submit' %>
<% end %>